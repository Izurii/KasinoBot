{
    "plugins": [
	],
    "recurseDepth": 10,
    "source": {
		"include": "./",
        "includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(^|\\/|\\\\)_",
		"exclude": [
            "node_modules",
            "JSDocTypes"
        ]
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}